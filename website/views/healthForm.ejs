<div class="jumbotron">
       <div class="container">
            <h1 class="display-4"><b>HEALTHCARE SERVICE<i class="fa fa-user-md" aria-hidden="true"></i></b></h1>
       </div>
</div>
<div class="ui container">


<div class="ui main text container segment">
    <div class="ui huge header">Health Checkup</div>
    
	<div>
	<h3>SYMPTOMS ANALYSER</h3>
	<ul>
	     <li>NAME: Diarrhea   --------------                       ID:50</li>
		 <li>NAME: Drowsiness --------------                       ID:43</li>
		 <li>NAME: Hearing loss --------------                     ID:206</li>
		 <li>NAME: Increased salivation --------------              ID:204</li>
		 <li>NAME: Itching of skin   --------------                ID:21</li>
		 <li>NAME: Lower abdominal pain --------------             ID:174</li>
		 <li>NAME: Mouth ulcers --------------                     ID:97</li>
		 <li>NAME: Sore throat --------------                      ID:13</li>
	</ul>
	   
	</div>
    
<form  action="/health" method="POST" class="ui form">
    
       
        
        <div class="field">
             <div class="eight wide field">
                <label>SYMPTOM </label>
                 <input name="sym" type="number" class="form-control" id="symptom" placeholder="Enter id">
            </div>
        </div>
		 <div class="field">
             <div class="eight wide field">
                <label>GENDER</label>
                <input name="gen" type="text" class="form-control"  placeholder="gender">
            </div>
        </div>
	
        <div class="field">
             <div class="eight  wide field">
                <label>AGE</label>
               <input name="year" type="number" class="form-control"  placeholder="year">
            </div>
        </div>
    <button class="positive ui button ">Submit <i class="icon edit "></i></button>
   
</form>

</div>
